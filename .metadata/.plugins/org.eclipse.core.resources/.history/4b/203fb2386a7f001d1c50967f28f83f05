function fn_submit() {
	var fn = document.frm;
	
	if(fn.custname.value == ""){ //input창에 값이 빈값인지 체크
		alert("회원성명이 입력되지 않았습니다.");
		fn.custname.focus(); //포커싱주기
		return false; //함수끝내기
	}
	
	if(fn.custphone.value == ""){ //input창에 값이 빈값인지 체크
		alert("회원번호가 입력되지 않았습니다.");
		fn.custphone.focus(); //포커싱주기
		return false; //함수끝내기
	}
	
	if(fn.custadress.value == ""){ //input창에 값이 빈값인지 체크
		alert("회원주소가 입력되지 않았습니다.");
		fn.custadress.focus(); //포커싱주기
		return false; //함수끝내기
	}
	
	if(fn.custjoindate.value == ""){ //input창에 값이 빈값인지 체크
		alert("가입일자가 입력되지 않았습니다.");
		fn.custjoindate.focus(); //포커싱주기
		return false; //함수끝내기
	}
	
	if(fn.custgrade.value == ""){ //input창에 값이 빈값인지 체크
		alert("고객등급이 입력되지 않았습니다.");
		fn.custgrade.focus(); //포커싱주기
		return false; //함수끝내기
	}
	
	if(fn.custcity.value == ""){ //input창에 값이 빈값인지 체크
		alert("도시코드가 입력되지 않았습니다.");
		fn.custcity.focus(); //포커싱주기
		return false; //함수끝내기
	}
	
	fn.submit(); //컨트롤러(서버)에 전송
}